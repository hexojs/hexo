{
  "extends": ["plugin:node/recommended", "hexo"],
  "root": true,
  "plugins": [ "node" ],
  "rules": {
    "one-var": 0,
    "operator-linebreak": [
      2,
      "after"
    ],
    "comma-spacing": [
      2,
      {
        "after": true
      }
    ],
    "node/no-unsupported-features": 0,
    "node/no-deprecated-api": 0
  }
}
