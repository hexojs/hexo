<div class="gallery">
  <div class="photoset">
    <% if (item.photos.length){ %>
      <% item.photos.forEach(function(photo){ %>
        <img src="<%- photo %>">
      <% }); %>
    <% } else { %>
      <img src="<%- item.photos %>">
    <% } %>
  </div>
  <div class="control">
    <div class="prev"></div>
    <div class="next"></div>
  </div>
</div>